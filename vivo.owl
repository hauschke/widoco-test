<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/minimal#"
         xml:base="http://example.org/minimal"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <owl:Ontology rdf:about="http://example.org/minimal">
    <rdfs:label xml:lang="en">Minimal Ontology</rdfs:label>
    <owl:versionInfo>1.0.0</owl:versionInfo>
  </owl:Ontology>

  <!-- Classes -->
  <owl:Class rdf:about="http://example.org/minimal#Person">
    <rdfs:label xml:lang="en">Person</rdfs:label>
    <rdfs:comment xml:lang="en">A human being</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/minimal#Organization">
    <rdfs:label xml:lang="en">Organization</rdfs:label>
    <rdfs:comment xml:lang="en">A group of people with a common goal</rdfs:comment>
  </owl:Class>

  <!-- Object property -->
  <owl:ObjectProperty rdf:about="http://example.org/minimal#memberOf">
    <rdfs:label xml:lang="en">member of</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/minimal#Person"/>
    <rdfs:range rdf:resource="http://example.org/minimal#Organization"/>
  </owl:ObjectProperty>

  <!-- Data property -->
  <owl:DatatypeProperty rdf:about="http://example.org/minimal#birthDate">
    <rdfs:label xml:lang="en">birth date</rdfs:label>
    <rdfs:domain rdf:resource="http://example.org/minimal#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
  </owl:DatatypeProperty>

  <!-- Individuals -->
  <owl:NamedIndividual rdf:about="http://example.org/minimal#Alice">
    <rdf:type rdf:resource="http://example.org/minimal#Person"/>
    <rdfs:label xml:lang="en">Alice</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/minimal#Bob">
    <rdf:type rdf:resource="http://example.org/minimal#Person"/>
    <rdfs:label xml:lang="en">Bob</rdfs:label>
  </owl:NamedIndividual>

  <owl:NamedIndividual rdf:about="http://example.org/minimal#ACME_Corp">
    <rdf:type rdf:resource="http://example.org/minimal#Organization"/>
    <rdfs:label xml:lang="en">ACME Corp</rdfs:label>
  </owl:NamedIndividual>

</rdf:RDF>
